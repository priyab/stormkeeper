{
	"name": "stormkeeper",
	"description": "stormkeeper provides support for managing Auth Tokens supplied by BiS component",
	"version": "0.1.0",
	"anyuthor": "Sivaprasath Busa",
	"homepage": "http://stormstack.org",
	"repository": {
		"type": "git",
		"url": "git://github.com/stormstack/stormkeeper.git"
	},
	"dependencies": {
		"zappajs": "0.3.x",
		"dirty": "0.9.x",
		"request": "2.9.x",
		"json-schema": "0.2.0",
		"lazy": "1.0.x",
		"async": "0.1.x",
		"fileops": "0.1.json3",
		"node-uuid": "1.3.x",
		"optimist": "0.6.0",
		"mux-demux": "3.7.8",
		"find-in-path":"0.0.x",
		"events":"1.0.x"
	},
	"devDependencies": {
		"coffee-script": ">=1.3"
	},
	"optionalDependencies": {},
	"engines": {
		"node": ">=0.6.x"
	},
	"main": "./lib/stormkeeper",
	"config": {
		"port": 5000,
		"logfile": "/var/log/stormkeeper.log",
		"datadir": "/var/stormkeeper",
		"stormflash": {
			"packages": [],  // dependant packages
			"plugins": "lib/api"
		}
	},
	"scripts": {
		"prepublish": "find src -name '*.coffee' | xargs coffee	-o lib -c",
		"start": "node lib/app.js"
	},
	"contributors": [
		"",
	]
}									]
