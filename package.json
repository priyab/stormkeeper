{
    "name": "stormkeeper",
	"description": "stormkeeper provides support for managing Auth Tokens supplied by BiS component",
	"version": "0.1.3",
	"author": "Sivaprasath Busa",
	"homepage": "http://stormstack.org",
	"repository": {
		"type": "git",
		"url": "git://github.com/stormstack/stormkeeper.git"
	},
	"dependencies": {
		"stormagent": "0.2.x",
		"request": "2.34.x",
		"json-schema": "0.2.0",
		"async": "0.8.x",
		"node-uuid": "1.3.x",
		"minimist":"0.1.0",
        "optimist":"0.6.0",
		"find-in-path":"0.0.x",
		"dirty": "0.9.x"
	},
	"devDependencies": {
		"coffee-script": ">=1.3"
	},
	"optionalDependencies": {},
	"engines": {
		"node": ">=0.6.x"
	},
	"main": "./lib/stormkeeper",
	"config": {
		"port": 8333,
		"logfile": "/var/log/stormkeeper.log",
		"storm": {
			"functions": [
				"agent.authorize",
				"agent.tokens",
				"agent.rules"
			],
			"plugins": [
				"lib/plugin"
			]
		}
	},
	"scripts": {
		"prepublish": "mkdir -p lib; coffee -o lib -c src",
		"start": "node lib/app"
	},
	"contributors": [
		"Peter K. Lee <peter@intercloud.net>"
	]
}
